{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Build, launch, and debug chN_eN",
      "type": "cppdbg",
      "request": "launch",
      "preLaunchTask": "build_chN_eN",
      "program": "${workspaceFolder}/chN/eN/bin/arm64/chN_eN",
      "args": [
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/chN/eN/bin/arm64",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}